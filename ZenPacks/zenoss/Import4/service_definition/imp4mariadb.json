{
    "servicePath": "/",
    "serviceDefinition" : {
        "Name": "Imp4MariaDB",
        "Description": "Mariadb environment and RRD converter worker for import4",
        "Launch": "manual",
        "Command": "bash -c \"while ! ( which rrdtool && [ -f /import4/pkg/bin/imp4mariadb.sh ] ); do sleep 10; done; /import4/pkg/bin/imp4mariadb.sh\"",
        "ImageID": "",
        "Tags": [
            "daemon"
            ],
        "Instances": {
            "Min": 1,
            "Max": 0,
            "Default": 0
        },
        "ChangeOptions": null,
        "Launch": "manual",
        "HostPolicy": "",
        "Hostname": "",
        "Privileged": false,
        "Context": {
            "global.conf.zodb-db": "zodb",
            "global.conf.zodb-db-type": "mysql",
            "global.conf.zodb-host": "127.0.0.1",
            "global.conf.zodb-port": "3307",
            "global.conf.zodb-admin-user": "root",
            "global.conf.zodb-admin-password": "",
            "global.conf.zodb-user": "zenoss",
            "global.conf.zodb-password": "zenoss"
        },
        "Endpoints": [
         {
          "Name": "controlplane_consumer",
          "Application": "controlplane_consumer",
          "PortNumber": 8444,
          "Protocol": "tcp",
          "Purpose": "import"
         },
         {
           "Name": "zencatalogservice",
           "Application": "zodb_zencatalogservice",
           "PortNumber": 8085,
           "Protocol": "tcp",
           "Purpose": "import"
         },
         {
           "Name": "rabbitmq",
           "Application": "rabbitmq.*",
           "Purpose": "import"
         },
         {
           "Name": "zep_mariadb",
           "Purpose": "import",
           "Protocol": "tcp",
           "PortNumber": 3306,
           "PortTemplate": "",
           "VirtualAddress": "",
           "Application": "zep_mariadb",
           "ApplicationTemplate": "",
           "AddressConfig": {
             "Port": 0,
             "Protocol": ""
           },
           "VHosts": null
         },
         {
           "Name": "zodb_mariadb",
           "Purpose": "import",
           "Protocol": "tcp",
           "PortNumber": 3307,
           "PortTemplate": "",
           "VirtualAddress": "",
           "Application": "zodb_mariadb",
           "ApplicationTemplate": "",
           "AddressConfig": {
             "Port": 0,
             "Protocol": ""
           },
           "VHosts": null
         },
         {
           "Name": "zep",
           "Purpose": "import",
           "Protocol": "tcp",
           "PortNumber": 8084,
           "PortTemplate": "",
           "VirtualAddress": "",
           "Application": "zep",
           "ApplicationTemplate": "",
           "AddressConfig": {
             "Port": 0,
             "Protocol": ""
           },
           "VHosts": null
         },
         {
           "Name": "zproxy",
           "Application": "zproxy",
           "PortNumber": 8080,
           "Protocol": "tcp",
           "Purpose": "import"
         }
       ],
       "Tasks": null,
       "LogFilters": null,
       "Volumes": [
         {
           "Owner": "mysql:mysql",
           "Permission": "0755",
           "ResourcePath": "mariadb-model",
           "ContainerPath": "/var/lib/mysql.model",
           "Type": ""
         },
         {
           "Owner": "mysql:mysql",
           "Permission": "0755",
           "ResourcePath": "mariadb-events",
           "ContainerPath": "/var/lib/mysql",
           "Type": ""
         },
         {
           "Owner": "zenoss:zenoss",
           "Permission": "0755",
           "ResourcePath": "zenoss-custom-patches-pc",
           "ContainerPath": "/opt/zenoss/.pc",
           "Type": ""
         },
         {
           "Owner": "zenoss:zenoss",
           "Permission": "0755",
           "ResourcePath": "zenoss-custom-patches",
           "ContainerPath": "/opt/zenoss/patches",
           "Type": ""
         },
         {
           "Owner": "zenoss:zenoss",
           "Permission": "0775",
           "ResourcePath": "zenoss-var-ext",
           "ContainerPath": "/opt/zenoss/var/ext",
           "Type": ""
         },
         {
           "Owner": "zenoss:zenoss",
           "Permission": "0775",
           "ResourcePath": "zenoss-var-flexera",
           "ContainerPath": "/opt/zenoss/var/flexera"
         },
         {
           "Owner": "zenoss:zenoss",
           "Permission": "0775",
           "ResourcePath": "zenoss-export",
           "ContainerPath": "/opt/zenoss/export",
           "Type": ""
         },
         {
           "Owner": "zenoss:zenoss",
           "Permission": "0755",
           "ResourcePath": ".ssh",
           "ContainerPath": "/home/zenoss/.ssh",
           "Type": ""
         },
         {
           "Owner": "zenoss:zenoss",
           "Permission": "0775",
           "ResourcePath": "var-zenpacks",
           "ContainerPath": "/var/zenoss"
         },
         {
           "Owner": "zenoss:zenoss",
           "Permission": "0755",
           "ResourcePath": "zencatalogservice",
           "ContainerPath": "/opt/zenoss/var/zencatalogservice",
           "Type": ""
         },
         {
           "Owner": "zenoss:zenoss",
           "Permission": "0775",
           "ResourcePath": "zeneventserver",
           "ContainerPath": "/opt/zenoss/var/zeneventserver",
           "Type": ""
         },
         {
           "Owner": "zenoss:zenoss",
           "Permission": "0777",
           "ResourcePath": "import4",
           "ContainerPath": "/import4"
         }
       ],
       "LogConfigs": [ ],
       "Snapshot": {
         "Pause": "",
         "Resume": ""
       },
       "ConfigFiles": {
         "/opt/zenoss/etc/global.conf": {
            "Filename": "/opt/zenoss/etc/global.conf",
            "Owner": "zenoss:zenoss",
            "Permissions": "660",
            "Content": "# Generated by serviced\n{{range $k,$v:=contextFilter . \"global.conf.\"}}{{$k}} {{$v}}\n{{end}}"
         },
         "/opt/zenoss/etc/zodb_db_imp4.conf": {
           "Filename": "/opt/zenoss/etc/zodb_db_imp4.conf",
           "Owner": "zenoss:zenoss",
           "Permissions": "660",
           "Content": "<mysql>\r\n host 127.0.0.1\r\n port 3307\r\n user zenoss\r\n passwd zenoss\r\n db zodb\r\n</mysql>"
         }
       },
       "RAMCommitment": "256M",
       "CPUCommitment": 1,
       "Runs": {
           "initialize": "cd $(dirname $(readlink /opt/zenoss/bin/import4)); ./install_pkg.sh && ./install_rrdtool.sh",
           "check" : "/import4/pkg/bin/imp4_check.sh",
           "model-database": "/import4/pkg/bin/imp4_run.sh model import --database",
           "model-catalog": "/import4/pkg/bin/imp4_run.sh model import --catalog",
           "model-zenmigrate": "/import4/pkg/bin/imp4_run.sh model import --zenmigrate",
           "model-zenpack": "/import4/pkg/bin/imp4_run.sh model import --zenpack",
           "model-verify": "/import4/pkg/bin/imp4_run.sh model verify",
           "events-database": "/import4/pkg/bin/imp4_run.sh events import --database",
           "events-index": "/import4/pkg/bin/imp4_run.sh events import --index",
           "perf-import": "/import4/pkg/bin/imp4_run.sh perf import",
           "perf-verify": "/import4/pkg/bin/imp4_run.sh perf verify",
           "perf-abort-cleanup": "(/import4/pkg/bin/abort_jobs.sh && exit 42)",
           "import4": "/import4/pkg/bin/imp4_run.sh"
       },
       "Actions": null,
       "HealthChecks": {
         "running": {
           "Script": "[ 1 -le $(pgrep -fla '/bin/bash /import4/pkg/bin/imp4mariadb.sh' | wc -l) ]",
           "Interval": 5.0
         },
         "dmd_ready": {
           "Script": "[ -f /import4/dmd_uuid.txt ]",
           "Interval": 5.0
         }
       },
       "Prereqs": [],
       "MonitoringProfile": {
         "MetricConfigs": null,
         "GraphConfigs": null,
         "ThresholdConfigs": null
       },
       "MemoryLimit": 0,
       "CPUShares": 0,
       "PIDFile": ""
    }
}
